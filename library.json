{
	"name": "Graphene",
	"version": "1.2.5",
	"description": "A simple library for creating graphical user interfaces",
	"keywords": "GUI, Graphical User Interface, Touchscreen, tft, ST7735, UI",
	"repository": {
		"type": "git",
		"url": "https://github.com/javiercont97/graphene.git"
	},
	"authors": [
		{
			"name": "Javier Contreras",
			"email": "javiercont97",
			"url": "https://github.com/javiercont97",
			"maintainer": true
		}
	],
	"license": "MIT",
	"homepage": "https://github.com/javiercont97/graphene",
	"dependencies": {
		"mbed-st/BSP_DISCO_F769NI": "0.0.0+sha.d13d7c447539",
		"adafruit/Adafruit ST7735 and ST7789 Library": "1.10.4"
	},
	"frameworks": [
		"mbed",
		"arduino",
		"freertos"
	],
	"platforms": "*",
	"headers": [
		"Graphene.h"
	],
	"build": {
		"flags": [
			"-std=gnu++2a",
			"-std=c++2a"
		],
		"unflags": [
			"-fno-rtti",
			"-std=gnu++14"
		]
	}
}